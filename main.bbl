    \field{title}{Distribution-Aware Margin Calibration for Semantic
  Segmentation in Images}
    \field{volume}{130}
    \field{journaltitle}{International Journal of Computer Vision}
    \field{month}{01}
    \field{year}{2022}
  \endentry

