    \verb 10.1016/j.media.2021.102259
    \endverb
    \field{issn}{1361-8415}
    \field{pages}{102259}
    \field{title}{Relax and focus on brain tumor segmentation}
    \field{volume}{75}
    \field{journaltitle}{Medical Image Analysis}
    \field{year}{2022}
  \endentry

